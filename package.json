{
  "name": "promisie",
  "version": "2.0.0-alpha.0",
  "description": "helpful add-on functionality for es6 Promises",
  "main": "dist/promisie.cjs.js",
  "module": "src/main.js",
  "esm": "dist/promisie.esm.js",
  "browser": "dist/promisie.umd.js",
  "web": "dist/promisie.web.js",
  "modules.root": "lib",
  "type": "module",
  "scripts": {
    "dev": "rollup -c -w",
    "test_simple": "node test/test.js",
    "benchmark": "node -r esm  benchmark/main_bmrk.js",
    "test": "jest --coverage",
    "doc": "esdoc",
    "compile": "tsc",
    "build": "tsc && rollup -c",
    "complete_build": "tsc && rollup -c && esdoc",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "promise",
    "promisify",
    "promisifyall",
    "es6",
    "es6 promise",
    "es6 class",
    "async",
    "series",
    "pipe",
    "compose",
    "generator",
    "try/catch",
    "map",
    "all",
    "spread",
    "parallel",
    "each",
    "dowhilst",
    "iterate",
    "retry",
    "finally"
  ],
  "author": "Jan Bialostok <janbialostok@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/repetere/promisie"
  },
  "license": "MIT",
  "analyze": true,
  "dependencies": {
    "babelify-es6-polyfill": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.4.3",
    "@repetere/esdoc-inject-gtm-plugin": "^0.1.0",
    "@types/chai": "^4.2.4",
    "@types/jest": "^24.0.19",
    "@types/jsdom-global": "^3.0.1",
    "@types/memory-cache": "^0.2.0",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-dom-factories": "^1.0.2",
    "@types/sinon": "^7.5.0",
    "@types/ua-parser-js": "^0.7.33",
    "@types/use-global-hook": "^0.1.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-replace-imports": "^1.0.2",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.7",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "esm": "3.2.22",
    "install": "^0.12.2",
    "jest": "^24.9.0",
    "microtime": "^3.0.0",
    "mocha": "^6.1.3",
    "mocha-sinon": "^2.1.0",
    "npm": "^6.9.0",
    "nyc": "^14.0.0",
    "prettier": "^1.18.2",
    "rollup": "^1.10.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-terser-js": "^1.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.2.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsd-jsdoc": "^2.4.0",
    "tslib": "^1.10.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  }
}
